<?xml version="1.0"?>
<JWM>
	<StartupCommand>
		./.fehbg &
		tint2 &
		numlockx &
		xcompmgr -c -t -5 -l -5 -r 3.5 -o .5 &
	</StartupCommand>

    <Group>
        <Option>centered</Option>
    </Group>

    <WindowStyle>
        <Width>1</Width>
        <Height>1</Height>
        <Outline>#1F292E</Outline>
        <Opacity>1</Opacity>
        <Active>
            <Outline>#1D272B</Outline>
        </Active>
    </WindowStyle>

    <Desktops width="2" height="1"></Desktops>

    <FocusModel>click</FocusModel>
    <SnapMode distance="10">border</SnapMode>
    <MoveMode coordinates="off">opaque</MoveMode>
    <ResizeMode coordinates="off">opaque</ResizeMode>

    <Key mask="A" key="Tab">nextstacked</Key>
    <Key mask="A" key="F4">close</Key>
    <Key mask="A" key="Right">rdesktop</Key>
    <Key mask="A" key="Left">ldesktop</Key>
    <Key mask="A" key="m">maximize</Key>
    <Key mask="A" key="i">minimize</Key>
    <Key mask="4" key="e">exec: rofi -show run</Key>
    <Key mask="4" key="f">exec: thunar</Key>
    <Key mask="4" key="t">exec: urxvt</Key>
    <Key mask="4" key="r">exec: jwm -restart</Key>
    <Key mask="4" key="p">exec: poweroff</Key>
    <Key key="Print">exec: scrot</Key>

    <Mouse context="root" button="1">exec: feh --randomize --bg-fill img</Mouse>
    <Mouse context="root" button="2">showdesktop</Mouse>
</JWM>
