[[ -z $SSH_TTY ]] && [[ -z $DISPLAY ]]  && setleds -D +num

export PS1=$(printf "\033[36m #@ \033[0m")
export EDITOR="micro"
export GOPATH="$HOME/git/go"

alias ls="ls --color"
alias la="ls -A"

push () {
	git add -A
	printf " commit: >> "
	read -r ans
	git commit -m "${ans:-"Nothing interesting to say"}"
	git push
}
